//设备运行状态
function change(obj) {
	var arr = $(".main_left_devstu_icon ul li img").removeClass("index_devstu_clickstyle");
	$(obj).addClass("index_devstu_clickstyle");
	if(obj.title==1){
		$(".main_left_devstu_img_img img").attr("src",config.serverPath+"/resources/images/web/4.0/index/body_devstu_hj.png");
	}else if(obj.title==2){
		$(".main_left_devstu_img_img img").attr("src",config.serverPath+"/resources/images/web/4.0/index/body_devstu_ims.png");
	}else if(obj.title==3){
		$(".main_left_devstu_img_img img").attr("src",config.serverPath+"/resources/images/web/4.0/index/body_devstu_light.png");
	}else if(obj.title==4){
		$(".main_left_devstu_img_img img").attr("src",config.serverPath+"/resources/images/web/4.0/index/body_devstu_video_open.png");
	}else if(obj.title==5){
		$(".main_left_devstu_img_img img").attr("src",config.serverPath+"/resources/images/web/4.0/index/body_devstu_mj.png");
	}else if(obj.title==6){
		$(".main_left_devstu_img_img img").attr("src",config.serverPath+"/resources/images/web/4.0/index/body_devstu_rfid.png");
	}
	
	
}
