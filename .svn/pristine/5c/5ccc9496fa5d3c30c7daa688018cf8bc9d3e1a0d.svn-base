var status = 0;
$(document).ready(function () {
	$(".detect_btn").addClass("ptjcclickstyle");
});
function updatakey(){
	if(status==0){
		$(".detect_cartoon").css({
			"display":"block",
		});
		$(".detect_btn").removeClass("ptjcclickstyle");
		status=1;
	}else{
		$(".detect_cartoon").css({
			"display":"none",
		});
		$(".detect_btn").addClass("ptjcclickstyle");
		status=0;
	}
	
}