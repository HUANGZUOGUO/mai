@charset "utf-8";
/* CSS Document */

body {
	font:12px/1.5 Tahoma, Helvetica, Arial, "MicroSoft Yahei", sans-serif; background:url(../../../images/web/bg.png) no-repeat; background-size:cover; background:#f6f6f6;
}

blockquote, body, button, code, dd, div, dl, dt, fieldset, form, h1, h2, h3, h4, h5, h6, input, legend, li, ol, p, pre, td, textarea, th, ul {
	margin:0;
	padding:0;
}
table {
	border-collapse:collapse;  /*重叠边框合并*/
	border-spacing:0;  /*边框间距为0*/
}
fieldset, img {
	border:none; /*边框为0*/
}
address, caption, cite, code, dfn, em, strong, th, var {
	font-style:normal;
	font-weight:400;
}
li {
	list-style:none;
}
caption, th {
	text-align:left;
}
h1, h2, h3, h4, h5, h6 {
	font-size:100%;
}




button, input, select, textarea {
*font-size:100%;
	font:100% 'MicroSoft YaHei',Tahoma, Helvetica, Arial, sans-serif;
}
table {
	font:100%;
}
a{ text-decoration:none; font-weight:normal;}
/*----------------------reset css ed       清除浏览器默认-----------------------*/


@-webkit-keyframes jb{
	
    0%{color: #666;}
	50%{color:#ff513f; background:#ff513f;}
	100%{color:#666;}
}
@-moz-keyframes jb {
    0%{color:#666;}
	50%{color:#ff513f;background:#ff513f; }
	100%{color:#666;}
}
@keyframes jb {
   0%{color:#666;}
	50%{color:#ff513f; background:#FF6063;}
	100%{color:#666;}
}

.jb{-webkit-animation: jb infinite 1.2s ease-out;
	-moz-animation: jb infinite 1.2s ease-out;
	-ms-animation: jb infinite 1.2s ease-out;
	animation: jb infinite 1.2s ease-out;
	}
	
	/*--------------------------背景颜色变化-----------------------------*/	
	

@-webkit-keyframes jb1{
	
    0%{color: #ff513f;}
	50%{color:#fff;}
	100%{color:#ff513f;}
}
@-moz-keyframes jb1{
    0%{color: #ff513f;}
	50%{color:#fff;}
	100%{color:#ff513f;}
}
@keyframes jb1{
    0%{color: #ff513f;}
	50%{color:#fff;}
	100%{color:#ff513f;}
}

.jb1{-webkit-animation: jb1 infinite 1.2s ease-out;
	-moz-animation: jb1 infinite 1.2s ease-out;
	-ms-animation: jb1 infinite 1.2s ease-out;
	animation: jb1 infinite 1.2s ease-out;
	}	
	/*--------------------------字体颜色变化-----------------------------*/	




@-webkit-keyframes jb2{
	
    0%{color: #666;}
	50%{color:#ff513f; transform: scale(1.2);transform: scaleX(-1);transition:all 0.6s ease;}
	100%{color:#666;}
}
@-moz-keyframes jb2 {
    0%{color:#666;}
	50%{color:#ff513f;transform: scale(1.2); transform: scaleX(-1); transform-origin:bottom right;ansition:all 0.6s ease;}
	100%{color:#666;}
}
@keyframes jb2 {
   0%{color:#666;}
	50%{color:#ff513f;transform: scale(1.2);transform: scaleX(-1);transition:all 0.6s ease;}
	100%{color:#666;}
}

.jb2{-webkit-animation: jb2 infinite 1.2s ease-out;
	-moz-animation: jb2 infinite 1.2s ease-out;
	-ms-animation: jb2 infinite 1.2s ease-out;
	animation: jb2 infinite 1.2s ease-out;
	}
	
	/*--------------------------翻转-----------------------------*/	
	
	
	
@-webkit-keyframes jb3{
	0%{transform: rotate(-360deg); transition:all 0.6s ease;}
}
@-moz-keyframes jb3 {
	0%{transform: rotate(-360deg); transition:all 0.6s ease;}
}
@keyframes jb3 {
	0%{transform: rotate(-360deg); transition:all 0.6s ease;}
}

.jb3{-webkit-animation: jb3 infinite 0.6s ease-out;
	-moz-animation: jb3 infinite 0.6s ease-out;
	-ms-animation: jb3 infinite 0.6s ease-out;
	animation: jb3 infinite 0.6s ease-out;
	}
	
	/*--------------------------旋转-----------------------------*/
	
@-webkit-keyframes jb4{
	
    0%{color: #666;}
	50%{color:#ff513f; transform: scale(1.2);transition:all 0.6s ease;}
	100%{color:#666;}
}
@-moz-keyframes jb4 {
    0%{color:#666;}
	50%{color:#ff513f;transform: scale(1.2);transition:all 0.6s ease;}
	100%{color:#666;}
}
@keyframes jb4 {
   0%{color:#666;}
	50%{color:#ff513f;transform: scale(1.2);transition:all 0.6s ease;}
	100%{color:#666;}
}

.jb4{-webkit-animation: jb4 infinite 1.2s ease-out;
	-moz-animation: jb4 infinite 1.2s ease-out;
	-ms-animation: jb4 infinite 1.2s ease-out;
	animation: jb4 infinite 1.2s ease-out;
	}
	/*--------------------------放大-----------------------------*/
	
	
@-webkit-keyframes jb5{
	
    0%{opacity:0.2;}
	50%{ opacity:1;}
	100%{opacity:0.2;}
}
@-moz-keyframes jb5 {
    0%{opacity:0.2;}
	50%{ opacity:1;}
	100%{opacity:0.2;}
}
@keyframes jb5 {
   0%{opacity:0.2;}
	50%{ opacity:1;}
	100%{opacity:0.2;}
}

.jb5{-webkit-animation: jb5 infinite 1.2s ease-out;
	-moz-animation: jb5 infinite 1.2s ease-out;
	-ms-animation: jb5 infinite 1.2s ease-out;
	animation: jb5 infinite 1.2s ease-out;
	}
