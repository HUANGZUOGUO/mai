var status = 0;
$(document).ready(function () {
	$(".maintenance_btn").addClass("xtwhclickstyle");
});
function updatakey(){
	if(status==0){
		$(".maintenance_cartoon").css({
			"display":"block",
		});
		$(".maintenance_btn").removeClass("xtwhclickstyle");
		status=1;
	}else{
		$(".maintenance_cartoon").css({
			"display":"none",
		});
		$(".maintenance_btn").addClass("xtwhclickstyle");
		status=0;
	}
	
}